<p id="notice"><%= notice %></p>

<h1>Listing Venue Layouts</h1>

<table>
  <thead>
    <tr>
      <th>Venue</th>
      <th>Name</th>
      <th>Capacity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @venue_layouts.each do |venue_layout| %>
      <tr>
        <td><%= venue_layout.venue_id %></td>
        <td><%= venue_layout.name %></td>
        <td><%= venue_layout.capacity %></td>
        <td><%= link_to 'Show', venue_layout %></td>
        <td><%= link_to 'Edit', edit_venue_layout_path(venue_layout) %></td>
        <td><%= link_to 'Destroy', venue_layout, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Venue layout', new_venue_layout_path %>
